<?xml version="1.0"?>
<dynamic_object>
  <diesel materials="units/world/props/street/plywood_fence/plywood_fence_io" orientation_object="rp_plywood_fence_door" />
  <sequence_manager file="units/world/props/street/plywood_fence/plywood_fence_door" />
  <animations>
  
    <animation_group name="anim">
      <object name="a_hinge" />
      <object name="a_handle" />
    </animation_group>
 </animations>
 
  <bodies>
    <!-- <body remove_ray_type="walk" name="static_body" enabled="true" template="static">
      <object name="rp_plywood_fence_door" />
      <object name="c_fence" collision_type="box" padding="-2" />
    </body>
	-->
    <body remove_ray_type="walk" name="new_body" enabled="true" template="animated">
      <object name="a_hinge" />
      <!-- <object name="c_handle" collision_type="mesh_mopp" /> -->
      <object name="c_door" collision_type="box" padding="-2" />
    </body>
  </bodies>
  
  <decal_surfaces default_material="metal">
    <decal_mesh name="dm_wood" enabled="true" material="wood" />
    <decal_mesh name="dm_metal_2" enabled="true" material="metal_hollow" />
    <decal_mesh name="dm_metal" enabled="true" material="metal_hollow" />
  </decal_surfaces>
  
  <graphics>
    <object name="g_handle" enabled="true"/>
    <object name="g_door" enabled="true"/>
    <object name="s_door" enabled="true"  shadow_caster="true"/>
    <object name="g_fence" enabled="true"/>
    <object name="s_fence" enabled="true"  shadow_caster="true"/>
  </graphics>
</dynamic_object> 