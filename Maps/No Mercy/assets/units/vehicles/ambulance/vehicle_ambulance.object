<dynamic_object>
    <diesel orientation_object="rp_ambulance" materials="units/vehicles/ambulance/vehicle_ambulance" />

<animations>
    <animation_group name="ag_tire_front_right">
      <object name="a_tire_front_right" />
    </animation_group>
    <animation_group name="ag_tire_front_left">
      <object name="a_tire_front_left" />
    </animation_group>
    <animation_group name="ag_tire_rear_right">
      <object name="a_tire_rear_right" />
    </animation_group>
    <animation_group name="ag_tire_rear_left">
      <object name="a_tire_rear_left" />
    </animation_group>
		
</animations>
	
    <bodies>
        <body enabled="true" template="static" name="static_body" >
            <object name="rp_ambulance" />
            <object collision_type="mesh_mopp" name="c_ambulance" />
        </body>
    </bodies>

	<decal_surfaces default_material="metal">
		<decal_mesh name="dm_ambulance" enabled="true" material="metal" />
		<decal_mesh name="dm_glass" enabled="true" material="glass_breakable" />
	</decal_surfaces>
	
    <graphics>
        <object enabled="true" name="g_ambulance"/>
        <object enabled="true" name="g_glass"/>
		<object enabled="true" name="s_ambulance" shadow_caster="true"/>
    </graphics>
</dynamic_object>
