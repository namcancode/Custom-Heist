<?xml version="1.0"?>
<dynamic_object>
	<diesel materials="units/world/architecture/hospital/hospital_scene_1" orientation_object="rp_rp" />
	<sequence_manager file="units/world/architecture/hospital/hospital_door_1" />
	<animations>
		<animation_group name="animation" >
			<object enabled="true" name="a_a" />
		</animation_group>
	</animations>
	<bodies>
		<body name="static_body" enabled="true" template="animated">
			<object name="a_a" />
			<object name="c_box" collision_type="box" padding="-2"/>
		</body>
	</bodies>

	<decal_surfaces default_material="wood" static="true">
		<decal_mesh name="dm_wood" enabled="true" material="wood" />
		<decal_mesh name="dm_metal" enabled="true" material="metal" />
		<decal_mesh name="dm_glass_unbreakable" enabled="true" material="glass_unbreakable" />
		<decal_mesh name="dm_handle" enabled="true" material="metal" />
	</decal_surfaces>

	<graphics>
		<object name="g_g" enabled="true"/>
		<object name="g_handle" enabled="true"/>
	</graphics>
</dynamic_object>