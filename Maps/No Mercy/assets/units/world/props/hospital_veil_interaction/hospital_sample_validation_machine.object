<?xml version="1.0"?>
<dynamic_object>
  <diesel materials="units/world/props/hospital_veil_interaction/hospital_veil_interaction" orientation_object="rp_hospital_sample_validation_machine" />
  <sequence_manager file="units/world/props/hospital_veil_interaction/hospital_sample_validation_machine"/>
  <animations>
		<animation_group name="ag_progress" enabled="false" >
			<object name="end" />
			<object name="progress" />
			<object name="a_spinner" />
		</animation_group>
		<animation_group name="ag_lid" enabled="false" >
			<object name="a_lid" />
		</animation_group>
	</animations>
	<bodies>
		<body name="static_body" enabled="true" collides_with_mover="false" template="static">
			<object name="rp_hospital_sample_validation_machine" />
			<object name="c_machine" collision_type="box" padding="-2"/>
		</body>
		<body name="lid_body" enabled="true" collides_with_mover="false" template="animated">
			<object name="a_lid" />
			<object name="c_lid" collision_type="box" padding="-2"/>
		</body>
	</bodies>
	<decal_surfaces>
		<decal_mesh name="g_machine" enabled="true" material="glass_no_decal" />
	</decal_surfaces>
	<graphics>
		<object name="g_machine" 			enabled="true"  />
		<object name="g_lid" 				enabled="true"  />
		<object name="g_spinner" 			enabled="true"  />
		<object name="g_veil" 				enabled="false"  />
		<object name="g_veil_dest" 			enabled="false"  />
		<object name="g_progrss" 			enabled="false"  />
		<object name="g_lid_outline" 		enabled="true"  />
		<object name="g_blood_base" 		enabled="false"/>
		<object name="g_blood_plane_lid" 	enabled="false"/>	
		<object name="g_blood_spinner" 		enabled="false"/>
	</graphics>
</dynamic_object> 