<?xml version="1.0" ?>
<dynamic_object>
	<diesel materials="units/payday2/architecture/ind/ind_ext_level/ind_ext_level_floor" orientation_object="rp_ind_ext_level_stairs_small" />
	<bodies>
		<body name="body_nav_walk" enabled="true" template="nav_walk">
			<object name="rp_ind_ext_level_stairs_small" />
			<object name="c_convex_mover" collision_type="convex" padding="-2.5" />
		</body>
        <body name="static_body" template="static" enabled="true" >
            <object name="rp_ind_ext_level_stairs_small"/>
			<object name="c_box_01" collision_type="box" padding="-2.5"/>
			<object name="c_box_02" collision_type="box" padding="-2.5"/>
			<object name="c_box_03" collision_type="box" padding="-2.5"/>
			<object name="c_box_04" collision_type="box" padding="-2.5"/>
			<object name="c_box_05" collision_type="box" padding="-2.5"/>
        </body>
    </bodies>
	
	<decal_surfaces default_material="plaster">
        <decal_mesh enabled="true" name="dm_plaster" material="plaster" />
    </decal_surfaces>

    <graphics>
        <lod_object name="g_g_lod">
            <object enabled="true" name="g_g" >
                <material materials="mat_ind_ext_level_floor" max_distance="3500"/>
                <material materials="mat_ind_ext_level_floor_lod1" />
            </object>
        </lod_object>
		<object enabled="true" name="s_s" shadow_caster="true"/>
    </graphics>
</dynamic_object> 



